<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestNG Regression Suite" verbose="4" thread-count="3" parallel="tests">
	
	
	<listeners>
		<listener
			class-name="qa.listeners.ExtentreportListener" />
		<listener
			class-name="qa.listeners.TestAllureListener" />
			
		<listener
			class-name="qa.listeners.AnnotationTransformer" />
		
		</listeners>

	<test name="Login Page Test">	
		<classes>
			<class name="qa.tests.LoginPageTest" />
		</classes>
	</test>
				
